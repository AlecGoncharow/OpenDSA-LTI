%html{:lang => "en"}
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:charset => "utf-8"}/
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
    %title jQuery UI Autocomplete - Combobox
    %link{:href => "//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css", :rel => "stylesheet"}/
    %script{:src => "https://code.jquery.com/jquery-1.12.4.js"}
    %script{:src => "https://code.jquery.com/ui/1.12.1/jquery-ui.js"}

    = stylesheet_link_tag "table_lti"
  -#{ :style => "display: inline-block; " }
  %body
    .ui-widget
      %strong Select Student:
      %select#combobox{:size => "10"}
        - @student_list.each do |q|
          %option{:value => q.id }
            #{q.first_name} #{q.last_name}

      %button#select.btn.btn-primary{:name => @course_id, :style => "float:right;"} Select
      %p
      #log
      %p
      #display_table


= javascript_include_tag('lti_table.js')
